{
    "version": 2,
    "builds": [
      {
        "src": "Server.js",
        "use": "@vercel/node"
      },
      {
        "src": "./images/**/*",
        "use": "@now/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "Server.js"
      },
      {
        "src": "/(.*)",
        "dest": "Server.js"
      }
    ]
  }
  